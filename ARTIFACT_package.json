  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "@openzeppelin/contracts": "^4.9.5",
    "body-parser": "^1.20.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "hardhat": "^2.19.2",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "chai": "^4.3.10",
    "@typechain/hardhat": "^8.0.3",
