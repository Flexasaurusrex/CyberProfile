{
  "name": "cyberpunk-pfp-minter",
  "version": "1.0.0",
  "description": "Farcaster mini app for minting cyberpunk-transformed profile pictures as NFTs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy:contract": "npx hardhat run scripts/deploy.js --network base",
    "verify:contract": "npx hardhat verify --network base",
    "test": "npx hardhat test"
  },
  "keywords": [
    "farcaster",
    "nft",
    "cyberpunk",
    "web3",
    "ethereum",
    "base"
