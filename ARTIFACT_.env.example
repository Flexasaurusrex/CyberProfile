# API Keys - Farcaster
NEYNAR_API_KEY=your_neynar_api_key_here

# API Keys - AI Image Generation
REPLICATE_API_TOKEN=your_replicate_api_token_here
STABILITY_API_KEY=your_stability_ai_key_here

# API Keys - IPFS
PINATA_API_KEY=your_pinata_api_key_here
